#include <stdio.h>

int main()
{
    long long int a,b,c,i,n;

    scanf("%lld", &n);

    for(i=0; i<n; i++){

        scanf("%lld%lld%lld", &a, &b, &c);

        if(a+b > c){

            if(a+c > b){

                if(b+c > a)printf("OK\n");

                else{printf("Wrong!!\n");}
            }

            else{printf("Wrong!!\n");}
        }

        else{printf("Wrong!!\n");}
    }

    return 0;
}

