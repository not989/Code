#include <stdio.h>
#include <math.h>

int main()
{
    long long int m,i;
    double l,w,r,ar1,ar2;

    scanf("%lld", &m);

    for(i=1; i<=m;i++){
        scanf("%lf", &l);

        w=(6*l)/10;
        r=l/5;
        ar1=l*w;
        ar2=M_PI*r*r;

        printf("%.2lf %.2lf\n", ar2,(ar1-ar2));
    }

    return 0;
}

