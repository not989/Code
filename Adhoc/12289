#include <bits/stdc++.h>
using namespace std;
int main()
{
    char str[20];
    long long int n,i,len;

    while(scanf("%lld",&n)==1)
    {
        getchar();
        while(n--)
        {
            gets(str);
            len=strlen(str);
            if(len==5)printf("3\n");

            else
            {
                if((str[0]=='o' && str[1]=='n' ||str[0]=='o' && str[2]=='e'||str[1]=='n' && str[2]=='e') && len==3)printf("1\n");
                else
                {
                    printf("2\n");
                }
            }
        }
    }

    return 0;
}

