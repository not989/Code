#include <stdio.h>

int main()
{
    long long int a,i,max,n,m,j;

    scanf("%lld",&m);

    for(j=1 ;j<=m; j++){

        scanf("%lld",&n);

        max=0;

        for(i=0; i<n ;i++){

            scanf("%lld", &a);

            if(a>max)max=a;
        }
        printf("Case %lld: %lld\n", j, max);
    }

    return 0;
}

